{
    "sitename": "liputan6",
    "url": "http://www.liputan6.com/sitemap.xml",
    "sitemapindex_regex": "^.*web.*sitemap.*\\.xml$",
    "url_regex": "^http.*\\/([a-zA-Z]*)\\.(?:liputan6\\.com\\/read\\/.+)$",
    "crawl_depth": 0,
    "title_tag": ["h1"],
    "title_attr": "class",
    "title_attr_val": "read-page--header--title",
    "article_tag": "div",
    "article_attr": "class",
    "article_attr_val": "article-content-body__item-content",
    "date_tag": "time",
    "date_attr": "class",
    "date_attr_val": "read-page--header--author__datetime",
    "date_regex": "",
    "normalize_date": true,
    "article_tag_replace": 
    {
        "<b>" : "<b>",
        "</b>" : "</b>",
        "</p>" : "<br><br>",
        "<br>": "<br>",
        "<br/>": "<br/>",
        "<strong>" : "<strong>",
        "</strong>" : "</strong>",
        "<em>" : "<em>",
        "</em>" : "</em>"
    },
    "article_regex_remove" : 
    [
        "(?:\\(Baca.*?\\))",
        "(?:<div.+class=\"video\".*?<\\/div>)",
        "(?:<strong>[\\s\\[]*Baca.*?>)"
    ]
}