{
    "sitename": "liputan6",
    "url": "http://www.liputan6.com",
    "sitemapindex_regex": "^.*web.*sitemap.*\\.xml$",
    "url_regex": "^(?:http|https)://([a-zA-Z]*)\\.liputan6\\.com\\/read\\/.+$",
    "crawl_depth": 1,
    "title_tag": ["h1"],
    "title_attr": "class",
    "title_attr_val": "read-page--header--title",
    "img_wrapper_tag": "picture",
    "img_wrapper_attr": "class",
    "img_wrapper_attr_val": "read-page--photo-gallery--item__picture",
    "prioritize_wrapper": false,
    "img_tag_attr": "class",
    "img_tag_attr_val": "read-page--photo-gallery--item__picture-lazyload",
    "img_tag_src_attr": "data-src",
    "allow_no_image": true,
    "author_tag": "span",
    "author_attr": "class",
    "author_attr_val": "read-page--header--author__name",
    "author_regex": "",
    "allow_default_author": false,
    "default_author_name": null,
    "date_tag": "time",
    "date_attr": "class",
    "date_attr_val": "read-page--header--author__datetime",
    "date_regex": "",
    "normalize_date": true,
    "article_tag": "div",
    "article_attr": "class",
    "article_attr_val": "article-content-body__item-content",
    "article_bs_remove": [
        {
            "tag": "div",
            "attr": "class",
            "attr_val": "baca-juga"
        }
    ],
    "article_tag_replace": 
    {
        "<b>" : "<b>",
        "</b>" : "</b>",
        "</p>" : "<br><br>",
        "<br>": "<br>",
        "<br/>": "<br/>",
        "<strong>" : "<strong>",
        "</strong>" : "</strong>",
        "<em>" : "<em>",
        "</em>" : "</em>"
    },
    "article_regex_remove" : 
    [
        "(?:\\(Baca.*?\\))",
        "(?:<div.+class=\"video\".*?<\\/div>)",
        "(?:<strong>[\\s\\[]*Baca.*?>)"
    ],
    "noox_config":
    {
        "db_sitename": "Liputan 6",
        "allow_no_image": true,
        "img_dir": "~/Proyek/noox-backend/public/img/news/",
        "url_regex": "^(?:http|https)://([a-zA-Z]*)\\.liputan6\\.com\\/read\\/.+$",
        "skip_when_no_category": true,
        "allow_default_category": false,
        "default_category": null,
        "categories":
        {
            "news": "national",
            "bisnis": "business",
            "health": "health",
            "bola": "sport",
            "tekno": "technology",
            "lifestyle": "lifestyle",
            "otomotif": "automotive"
        }
    }
}