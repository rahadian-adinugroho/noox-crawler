{
    "sitename": "detik",
    "url": "http://detik.com",
    "url_regex": "^http.*\\/([a-zA-Z]*)\\.(?:.*\\/read\\/.*|.*\\/d-[0-9]*\\/.*)$",
    "article_tag": "div",
    "article_attr": "class",
    "article_attr_val": "detail_text",
    "article_tag_replace": 
    {
        "<br>": "<br>",
        "<br/>": "<br/>",
        "<strong>" : "<strong>",
        "</strong>" : "</strong>",
        "<b>" : "<b>",
        "</b>" : "</b>",
        "<em>" : "<em>",
        "</em>" : "</em>"
    },
    "article_regex_remove" : 
    [
        "(?:<table.*?<\\/table>)",
        "(?:\\[.+?Video.*?\\])"
    ]
}