{
    "sitename": "detik",
    "url": "http://detik.com",
    "url_regex": "^http.*\/([a-zA-Z]*)\\.(?:.*\\/read\\/.*|.*\\/d-[0-9]*\\/.*|.*\\/berita\\/.*)$",
    "crawl_depth": 1,
    "title_tag": "h1",
    "title_attr": null,
    "title_attr_val": null,
    "article_tag": "div",
    "article_attr": "class",
    "article_attr_val": "detail_text|text_detail",
    "date_tag": ["div", "span"],
    "date_attr": "class",
    "date_attr_val": "date",
    "date_regex": "",
    "normalize_date": true,
    "article_tag_replace": 
    {
        "<br>": "<br>",
        "<br/>": "<br/>",
        "<strong>" : "<strong>",
        "</strong>" : "</strong>",
        "<b>" : "<b>",
        "</b>" : "</b>",
        "<em>" : "<em>",
        "</em>" : "</em>"
    },
    "article_regex_remove" : 
    [
        "(?:<table.*?<\\/table>)",
        "(?:\\[.+?Video.*?\\])"
    ]
}