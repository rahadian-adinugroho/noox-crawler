{
    "sitename": "kompas",
    "url": "http://kompas.com/sitemap.php",
    "sitemapindex_regex": "",
    "url_regex": "^(?:http|https)://([a-zA-Z]*)\\.kompas\\.com\\/read\\/.+$",
    "crawl_depth": 0,
    "title_tag": ["h1"],
    "title_attr": "class",
    "title_attr_val": "read__title",
    "author_tag": "div",
    "author_attr": "class",
    "author_attr_val": "read__author",
    "author_regex": "",
    "allow_default_author": false,
    "default_author_name": null,
    "article_tag": "div",
    "article_attr": "class",
    "article_attr_val": "read__content",
    "date_tag": "div",
    "date_attr": "class",
    "date_attr_val": "read__time",
    "date_regex": "",
    "normalize_date": false,
    "article_bs_remove": [],
    "article_tag_replace": 
    {
        "</p>" : "<br><br>",
        "<strong>" : "<strong>",
        "</strong>" : "</strong>"
    },
    "article_regex_remove" : 
    [
    	"(?:\\(Baca.*?\\))",
    	"(?:<div.+class=\"video\".*?<\\/div>)",
        "(?:<strong>[\\s\\[]*Baca.*?>)"
    ]
}